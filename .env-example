# TyTuX Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Gemini API Key
# Get yours at: https://aistudio.google.com/apikey
GEMINI_API_KEY="your_gemini_api_key_here"

# New Relic API Key (User key format: NRAK-XXXXXXXX)
# Get yours from your New Relic account settings
NEW_RELIC_USER_API_KEY="your_new_relic_user_api_key_here"

# Your New Relic Account ID (numeric value)
NEW_RELIC_ACCOUNT_ID="your_account_id_here"

# New Relic API Endpoint (usually no need to change this)
NEW_RELIC_API_ENDPOINT="https://api.newrelic.com/graphql"

